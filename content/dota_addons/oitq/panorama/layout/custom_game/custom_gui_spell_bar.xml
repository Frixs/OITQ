<root>
  <styles>
    <include src="file://{resources}/styles/custom_game/custom_gui_spell_bar.css" />
  </styles>
  <scripts>
    <include src="file://{resources}/scripts/custom_game/custom_gui_spell_bar.js" />      
  </scripts>
  <Panel class="BaseHud" hittest="false">
    <Panel id="spell-wrapper" hittest="false">
        <Panel id="LPAdd-box">
            <Label id="LPAdd-text" text="+0" onactivate="OnLevelUpClicked()" />
        </Panel>
        <!-- Primary -->
        <Panel id="abilityBox-0" class="abilityBox abilitySlot-bigger" hittest="false">
            <Button class="abilityButton"
                    onmouseover="AbilityShowTooltip(0)"
                    onmouseout= "AbilityHideTooltip(0)"
                    onactivate=    "ActivateAbility(0)"
                    >
                <DOTAAbilityImage class="abilityIcon" abilityname="shuriken_spell" />
                <Panel class="cooldownBox">
                    <Label class="cooldownTime" text="0.0" />
                </Panel>
                <Panel class="learnModeBox" />
                <Panel class="silenceBox" />
                <Panel class="passiveBox" />
                <Label id="shurikenCharges" text="0" />
            </Button>
            <Panel class="abilityIconFrame" hittest="false">
                <Label class="keybindAbbr" text="X" />
            </Panel>
        </Panel>
        <!-- Secondary -->
        <Panel id="abilityBox-1" class="abilityBox abilitySlot-smaller" hittest="false">
